{
  "compilerOptions": {
    "sourceMap": true,
    "module": "CommonJS",
    "target": "ES6",
    "moduleResolution": "Node",
    "alwaysStrict": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}
